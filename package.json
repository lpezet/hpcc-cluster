{
  "name": "@lpezet/hpcc-cluster",
  "version": "1.3.1",
  "description": "HPCC Cluster management in JS",
  "main": "",
  "scripts": {
    "lint": "eslint lib/ tests/",
    "fix": "eslint lib/ tests/ --fix",
    "pretest": "npm run lint",
    "test": "nyc mocha --bail --check-leaks --reporter spec tests/ -name \"*-test.js\" --recursive",
    "posttest": "nyc check-coverage",
    "integration-test": "mocha ./integration-tests/*-test.js",
    "docs-gen": "doctoc README.md Mods.md --github --no-title",
    "test-watch": "mocha --watch --reporter=spec",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lpezet/hpcc-cluster.git"
  },
  "keywords": [
    "hpcc-systems",
    "hpcc"
  ],
  "author": "Luke Pezet <lpezet@gmail.com>",
  "bugs": {
    "url": "https://github.com/lpezet/hpcc-cluster/issues"
  },
  "nyc": {
    "include": "lib",
    "check-coverage": true,
    "lines": 90,
    "statements": 90,
    "functions": 90,
    "branches": 80,
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "license": "MIT",
  "engines": {
    "node": ">= 0.12.4"
  },
  "dependencies": {
    "@lpezet/etl-js": "^1.2.0",
    "aws-sdk": "^2.1354.0",
    "external-ip": "^0.2.4",
    "js-yaml": "^3.13.1",
    "moniker": "^0.1.2",
    "mustache": "^2.3.0",
    "netaddr": "^1.1.0",
    "node-ssh": "^4.1.0",
    "opn": "^5.5.0",
    "request": "^2.87.0",
    "ssh2": "^0.8.2",
    "sync-request": "^6.0.0",
    "underscore": "^1.9.1",
    "velocity": "^0.7.2",
    "velocityjs": "^1.0.1",
    "when": "^3.7.7",
    "yaml-cfn": "^0.2.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-spies": "^1.0.0",
    "crypto": "^1.0.1",
    "doctoc": "^1.4.0",
    "eslint": "^5.15.3",
    "mocha": "^5.0.0",
    "nyc": "^14.1.1",
    "sinon": "^4.1.6"
  }
}
